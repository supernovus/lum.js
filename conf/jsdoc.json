{
  "tags": 
  {
    "allowUnknownTags": true
  },
  "opts": 
  {
    "destination": "./docs/api"
  },
  "plugins": 
  [
    "plugins/markdown"
  ],
  "templates": 
  {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": 
    {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "YYYY-MM-DD, hh:mm:ss"
  }
}
